// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "alt+left",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "ctrl+pageup",
        "command": "-workbench.action.previousEditor"
    },
    {
        "key": "alt+right",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "ctrl+pagedown",
        "command": "-workbench.action.nextEditor"
    },
    {
        "key": "ctrl+q",
        "command": "editor.action.deleteLines",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+[Period]",
        "command": "macros.comment",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+[Period]",
        "command": "-editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "escape",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "!editorTextFocus"
    },
    {
        "key": "alt+1",
        "command": "workbench.view.explorer"
    },
    {
        "key": "alt+1",
        "command": "-workbench.action.openEditorAtIndex1"
    },
    {
        "key": "alt+2",
        "command": "workbench.view.search",
        "when": "!searchViewletVisible"
    },
    {
        "key": "alt+2",
        "command": "-workbench.action.openEditorAtIndex2"
    },
    {
        "key": "alt+3",
        "command": "workbench.view.scm"
    },
    {
        "key": "alt+4",
        "command": "workbench.view.debug"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+shift+d",
        "command": "editor.action.changeAll",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+c",
        "command": "editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+b",
        "command": "editor.action.goToDeclaration",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
        "key": "ctrl+shift+b",
        "command": "editor.action.goToTypeDefinition"
    },
    {
        "key": "ctrl+shift+b",
        "command": "-workbench.action.tasks.build"
    },
    {
        "key": "ctrl+t",
        "command": "workbench.action.gotoSymbol"
    },
    {
        "key": "ctrl+shift+alt+n",
        "command": "workbench.action.showAllSymbols"
    },
    {
        "key": "ctrl+l",
        "command": "editor.action.smartSelect.grow",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+[KeyM]",
        "command": "editor.debug.action.toggleBreakpoint",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+w",
        "command": "workbench.action.closeActiveEditor"
    },
    {
        "key": "ctrl+alt+z",
        "command": "git.clean"
    },
    {
        "key": "ctrl+alt+q",
        "command": "workbench.action.quit"
    },
    {
        "key": "ctrl+q",
        "command": "-workbench.action.quit"
    },
    {
        "key": "ctrl+shift+o",
        "command": "workbench.action.files.openFolder"
    },
    {
        "key": "alt+g",
        "command": "git.openChange"
    },
    {
        "key": "ctrl+shift+k",
        "command": "git.stash"
    },
    {
        "key": "ctrl+alt+k",
        "command": "git.stashPop"
    },
    {
        "key": "ctrl+shift+j",
        "command": "extension.saveTabSet"
    },
    {
        "key": "ctrl+alt+j",
        "command": "extension.openTabSet"
    },
    {
        "key": "alt+d",
        "command": "workbench.action.debug.start",
        "when": "!inDebugMode"
    },
    {
        "key": "ctrl+shift+alt+d",
        "command": "macros.debugStop",
        "when": "inDebugMode"
    },
    {
        "key": "alt+up",
        "command": "workbench.action.splitEditor"
    },
    {
        "key": "ctrl+shift+tab",
        "command": "workbench.action.focusFirstEditorGroup"
    },
    {
        "key": "ctrl+tab",
        "command": "workbench.action.focusSecondEditorGroup"
    },
    {
        "key": "alt+w",
        "command": "workbench.action.closePanel"
    },
    {
        "key": "alt+t",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "alt+p",
        "command": "workbench.actions.view.problems"
    },
    {
        "key": "alt+f",
        "command": "filesExplorer.findInFolder"
    },
    {
        "key": "alt+m",
        "command": "workbench.files.action.showActiveFileInExplorer"
    },
    {
        "key": "alt+o",
        "command": "workbench.action.output.toggleOutput"
    },
    {
        "key": "ctrl+k ctrl+h",
        "command": "-workbench.action.output.toggleOutput"
    },
    {
        "key": "ctrl+p",
        "command": "editor.action.marker.next",
        "when": "editorFocus && !editorReadonly"
    },
    {
        "key": "alt+q",
        "command": "workbench.action.closeWindow"
    },
    {
        "key": "ctrl+shift+w",
        "command": "-workbench.action.closeWindow"
    },
    {
        "key": "ctrl+shift+down",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "-editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+up",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+up",
        "command": "-editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "workbench.action.closeEditorsInGroup"
    },
    {
        "key": "ctrl+k w",
        "command": "-workbench.action.closeEditorsInGroup"
    },
    {
        "key": "ctrl+enter",
        "command": "editor.action.quickFix",
        "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+[Comma]",
        "command": "-editor.action.quickFix",
        "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+x",
        "command": "workbench.debug.action.toggleRepl"
    },
    {
        "key": "ctrl+shift+y",
        "command": "-workbench.debug.action.toggleRepl"
    },
    {
        "key": "ctrl+shift+alt+down",
        "command": "workbench.action.debug.stepOver",
        "when": "inDebugMode"
    },
    {
        "key": "f10",
        "command": "-workbench.action.debug.stepOver",
        "when": "inDebugMode"
    },
    {
        "key": "ctrl+shift+alt+down",
        "command": "-editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+right",
        "command": "workbench.action.debug.stepInto",
        "when": "inDebugMode"
    },
    {
        "key": "f11",
        "command": "-workbench.action.debug.stepInto",
        "when": "inDebugMode"
    },
    {
        "key": "ctrl+shift+alt+left",
        "command": "workbench.action.debug.stepOut",
        "when": "inDebugMode"
    },
    {
        "key": "shift+f11",
        "command": "-workbench.action.debug.stepOut",
        "when": "inDebugMode"
    },
    {
        "key": "alt+i",
        "command": "surround.with",
        "when": "editorHasSelection"
    },
    {
        "key": "ctrl+shift+t",
        "command": "-surround.with",
        "when": "editorHasSelection"
    },
    {
        "key": "alt+c",
        "command": "workbench.action.debug.continue",
        "when": "inDebugMode"
    },
    {
        "key": "f5",
        "command": "-workbench.action.debug.continue",
        "when": "inDebugMode"
    },
    {
        "key": "alt+v",
        "command": "workbench.debug.action.focusVariablesView"
    },
    {
        "key": "alt+b",
        "command": "breadcrumbs.focusAndSelect",
        "when": "breadcrumbsVisible"
    },
    {
        "key": "ctrl+shift+[IntlBackslash]",
        "command": "-breadcrumbs.focusAndSelect",
        "when": "breadcrumbsVisible"
    },
    {
        "key": "ctrl+u",
        "command": "references-view.find",
        "when": "editorHasReferenceProvider"
    },
    {
        "key": "shift+alt+f12",
        "command": "-references-view.find",
        "when": "editorHasReferenceProvider"
    },
    {
        "key": "ctrl+u",
        "command": "-cursorUndo",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+i",
        "command": "references-view.findImplementations"
    },
    {
        "key": "ctrl+i",
        "command": "-expandLineSelection",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+g ctrl+alt+g",
        "command": "extension.openCodeBlock"
    },
    {
        "key": "ctrl+alt+g ctrl+alt+n",
        "command": "extension.createCodeBlock"
    },
    {
        "key": "alt+h",
        "command": "gitlens.views.fileHistory:scm.focus"
    },
    {
        "key": "alt+c",
        "command": "gitlens.views.compare:gitlens.focus",
        "when": "!inDebugMode"
    },
    {
        "key": "alt+r",
        "command": "gitlens.views.repositories:gitlens.focus"
    },
    {
        "key": "ctrl+alt+enter",
        "command": "git.stage"
    },
    {
        "key": "alt+z",
        "command": "git.revertSelectedRanges"
    },
    {
        "key": "alt+enter",
        "command": "git.stageSelectedRanges"
    },
    {
        "key": "ctrl+shift+e",
        "command": "workbench.action.openPreviousEditorFromHistory"
    },
    {
        "key": "ctrl+m",
        "command": "workbench.action.keepEditor"
    },
    {
        "key": "ctrl+k enter",
        "command": "-workbench.action.keepEditor"
    },
    {
        "key": "ctrl+r",
        "command": "editor.action.showDefinitionPreviewHover"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.openRecent"
    },
    {
        "key": "ctrl+r shift+x",
        "command": "-npm-script.terminate-script"
    },
    {
        "key": "ctrl+r l",
        "command": "-npm-script.showOutput"
    },
    {
        "key": "ctrl+r t",
        "command": "-npm-script.test"
    },
    {
        "key": "ctrl+r shift+r",
        "command": "-npm-script.run"
    },
    {
        "key": "ctrl+r r",
        "command": "-npm-script.rerun-last-script"
    },
    {
        "key": "ctrl+enter",
        "command": "search.action.replace",
        "when": "matchFocus && replaceActive && searchViewletVisible"
    },
    {
        "key": "ctrl+shift+1",
        "command": "-search.action.replace",
        "when": "matchFocus && replaceActive && searchViewletVisible"
    },
    {
        "key": "ctrl+enter",
        "command": "search.action.replaceAllInFile",
        "when": "fileMatchFocus && replaceActive && searchViewletVisible"
    },
    {
        "key": "ctrl+shift+1",
        "command": "-search.action.replaceAllInFile",
        "when": "fileMatchFocus && replaceActive && searchViewletVisible"
    },
    {
        "key": "ctrl+enter",
        "command": "search.action.replaceAllInFolder",
        "when": "folderMatchFocus && replaceActive && searchViewletVisible"
    },
    {
        "key": "ctrl+shift+1",
        "command": "-search.action.replaceAllInFolder",
        "when": "folderMatchFocus && replaceActive && searchViewletVisible"
    },
    {
        "key": "ctrl+shift+z",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "ctrl+shift+z",
        "command": "-editor.action.customEditor.redo",
        "when": "focusedCustomEditorIsEditable && !inputFocus"
    },
    {
        "key": "ctrl+shift+z",
        "command": "-redo",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+y",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "ctrl+8",
        "command": "-workbench.action.navigateForward"
    },
    {
        "key": "ctrl+shift+6",
        "command": "-workbench.action.navigateForward"
    },
    {
        "key": "ctrl+8",
        "command": "-workbench.action.navigateForward"
    },
    {
        "key": "ctrl+alt+f",
        "command": "search.action.openNewEditor"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.pinEditor",
        "when": "config.workbench.editor.showTabs && !editorSticky"
    },
    {
        "key": "ctrl+k shift+enter",
        "command": "-workbench.action.pinEditor",
        "when": "config.workbench.editor.showTabs && !editorSticky"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.unpinEditor",
        "when": "config.workbench.editor.showTabs && editorSticky"
    },
    {
        "key": "ctrl+k shift+enter",
        "command": "-workbench.action.unpinEditor",
        "when": "config.workbench.editor.showTabs && editorSticky"
    },
    {
        "key": "ctrl+j",
        "command": "-workbench.action.togglePanel"
    }
]


